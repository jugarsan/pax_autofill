{
    "manifest_version": 2,

    "name": "Pax autocomplete extension",
    "description": "This extension is for checkout_form page autocomplete",
    "version": "1.0",

    "permissions": [
        "tabs",
        "*://*.switchfly.com/"
    ],

    "background": {
        "scripts": ["background.js"]
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["/js/pax_fill.js", "/js/utils.js"]
        }
    ],

    "browser_action":{
        "default_icon": "icon.png"
    }
}